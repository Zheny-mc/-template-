<?xml version="1.0" encoding="UTF-8"?>
<beans  xmlns="http://www.springframework.org/schema/beans"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:context="http://www.springframework.org/schema/context"
        xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context.xsd">
    
    <context:property-placeholder location="classpath:configPanel.properties" />
    
	<bean id="testApp" class="springhw.TestApp">
        <property name="properties">
            <props>
                <prop key="first">First value</prop>
                <prop key="second">Second value</prop>
            </props>
        </property>
        <property name="list">
            <list>
                <value>${circle.x}</value>
                <value>${key.Q}</value>
            </list>
        </property>
        <property name="set">
            <set>
                <value>First</value>
                <value>First</value>
                <value>Second</value>
            </set>
        </property>
        <property name="map">
            <map>
                <entry key="firstKey" value="First value" />
                <entry key="secondKey" value="Second value" />
            </map>
        </property>
    </bean>

	
	
</beans>